{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html"
        ],
        "versionedFiles": [
          "/*.bundle.css",
          "/*.bundle.js",
          "/*.chunk.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-chuck",
      "urls": [
        "https://lmepwachucknorris.azurewebsites.net/**"
      ],
      "cacheConfig":{
        "strategy":"freshness",
        "maxAge":"3d",
        "timeout":"1m",
        "maxSize":100
      }
    }
  ]
}